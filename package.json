{
  "name": "admin-backend",
  "version": "1.0.0",
  "description": "Admin backend with MongoDB only (Firebase removed). Uses unified security middleware and validation.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "node scripts/test-retry.js",
    "start:admin": "node index-admin.js",
    "start:user": "node index-user.js",
    "dev:admin": "nodemon index-admin.js",
    "dev:user": "nodemon index-user.js",
    "dev:both": "concurrently \"npm run dev:admin\" \"npm run dev:user\"",
    "start:both": "concurrently \"npm run start:admin\" \"npm run start:user\""
  },
  "keywords": [
    "admin",
    "backend",
    "mongodb",
    "railway",
    "react",
    "expo"
  ],
